/*Implemente um programa que indique se um inteiro lido é zero ou não*/

#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main() {
    int num;
    printf("Digite um numero: \n");
    scanf("%d",&num);
    
    if (num != 0){
        printf("Voce nao digitou zero");
    } else{
        printf("Voce digitou zero");
    }


    return 0;
}
